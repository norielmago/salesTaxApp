<!DOCTYPE html>
<html>
  <head>
    <title>SalesTaxes</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    '/assets/stylesheets/bootstrap/bootstrap.min', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag    '/assets/stylesheets/toastr/toastr.min', media: 'all', 'data-turbolinks-track' => true %>

    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag '/assets/javascripts/bootstrap/jquery-3.3.1.slim.min', 'data-turbolinks-track' => true%>
    <%= javascript_include_tag '/assets/javascripts/bootstrap/bootstrap.min', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag '/assets/javascripts/bootstrap/popper.min', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag '/assets/javascripts/toastr/toastr.min', 'data-turbolinks-track' => true %>

  </head>

  <body>
    <div class="container-fluid" style="margin-top: 60px;">
      <%= yield %>
    </div>
  </body>
</html>
